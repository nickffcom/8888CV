{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "watch-dev": "concurrently 'yarn eslint:watch' 'mix watch'",
        "eslint:watch": "npx esw -w './resources/assets/js/' -c './.eslintrc.js' --ext '.js,.vue' --watch-delay 0 --fix --cache --clear --changed './resources/assets/js/'",
